{
  "name": "soulnext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "homepage": "https://soulnext.readeral.now.sh",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@mdx-js/react": "^1.5.1",
    "@newfrontdoor/audio-player": "^0.1.6",
    "@newfrontdoor/calendar": "^2.2.0",
    "@newfrontdoor/sanity-block-components": "0.1.1",
    "@react-google-maps/api": "^1.8.0",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/client": "^0.147.0",
    "@sanity/image-url": "^0.140.15",
    "@theme-ui/components": "^0.2.50",
    "@zeit/next-css": "^1.0.1",
    "config": "^3.2.4",
    "dotenv": "^8.2.0",
    "embla-carousel-react": "^1.2.7",
    "emotion-theming": "^10.0.19",
    "final-form": "^4.18.6",
    "got": "^10.0.4",
    "ky": "^0.16.1",
    "ky-universal": "^0.3.0",
    "micro-cors": "^0.1.1",
    "next": "9.1.5",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^0.19.0",
    "next-images": "^1.2.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-final-form": "^6.3.3",
    "react-icons": "^3.8.0",
    "react-spring": "^8.0.27",
    "theme-ui": "^0.2.49",
    "xmlbuilder": "^13.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "eslint": "^6.7.2",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prettier": "^1.19.1",
    "xo": "^0.25.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "xo": {
    "extends": [
      "xo-react",
      "plugin:jest/recommended"
    ],
    "parser": "babel-eslint",
    "prettier": true,
    "space": true,
    "plugins": [
      "emotion",
      "jest"
    ],
    "rules": {
      "emotion/jsx-import": "error",
      "emotion/no-vanilla": "error",
      "emotion/import-from-emotion": "error",
      "emotion/styled-import": "error"
    },
    "overrides": [
      {
        "files": "**/__tests__/**/*.js",
        "rules": {
          "import/no-unassigned-import": [
            "error",
            {
              "allow": [
                "@testing-library/jest-dom/extend-expect"
              ]
            }
          ]
        }
      }
    ]
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
