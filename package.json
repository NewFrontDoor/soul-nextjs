{
  "name": "soulnext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "homepage": "https://soulnext.readeral.now.sh",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@mdx-js/react": "^1.5.1",
    "@newfrontdoor/audio-player": "^0.3.0",
    "@newfrontdoor/footer": "^1.1.1",
    "@newfrontdoor/link": "^0.1.2",
    "@newfrontdoor/sanity-block-components": "0.1.1",
    "@react-google-maps/api": "^1.9.5",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/client": "^1.149.16",
    "@sanity/image-url": "^0.140.15",
    "@theme-ui/components": "^0.3.1",
    "config": "^3.2.4",
    "embla-carousel-react": "^2.0.3",
    "emotion-theming": "^10.0.19",
    "final-form": "^4.18.6",
    "got": "^11.3.0",
    "ky": "^0.23.0",
    "micro-cors": "^0.1.1",
    "next": "^9.5.4",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.1.0",
    "next-images": "^1.2.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-final-form": "^6.3.3",
    "react-icons": "^3.8.0",
    "react-spring": "^8.0.27",
    "theme-ui": "^0.3.1",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-react-hooks": "^4.0.8",
    "xo": "^0.32.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "xo": {
    "extends": [
      "xo-react"
    ],
    "parser": "babel-eslint",
    "prettier": true,
    "space": true,
    "envs": [
      "browser"
    ],
    "plugins": [
      "emotion"
    ],
    "rules": {
      "react/jsx-pascal-case": "off",
      "import/no-unassigned-import": [
        "error",
        {
          "allow": [
            "**/*.css"
          ]
        }
      ],
      "emotion/jsx-import": "error",
      "emotion/no-vanilla": "error",
      "emotion/import-from-emotion": "error",
      "emotion/styled-import": "error"
    }
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
